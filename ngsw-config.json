{
  "$schema": "https://raw.githubusercontent.com/angular/angular/20.1.x/packages/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/offline.html",
          "/manifest.webmanifest",
          "/**/*.css",
          "/**/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/i18n/**",
          "/image/**",
          "/icons/**",
          "/logo/**",
          "/assets/**",
          "/*.(png|jpg|jpeg|svg|webp|gif)"
        ]
      }
    }
  ],
  "navigationUrls": ["/**", "!/**/*.*", "!/**/*__*", "!/api/**"],
  "navigationRequestStrategy": "performance"
}
