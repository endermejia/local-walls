{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/local-walls/browser",
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(ngsw-worker.js|manifest.webmanifest)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/api/8anu/:path*", "destination": "https://www.8a.nu/:path*" },
    { "source": "/(.*)", "destination": "/" }
  ]
}
