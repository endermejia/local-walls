{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/local-walls/browser",
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/ngsw-worker.js",
      "headers": [
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        },
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    },
    {
      "source": "/ngsw.json",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    },
    {
      "source": "/index.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/manifest.webmanifest",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/api/8anu/:path*", "destination": "https://www.8a.nu/:path*" },
    { "source": "/image/(.*)", "destination": "/image/$1" },
    { "source": "/(.*)", "destination": "/" }
  ]
}
